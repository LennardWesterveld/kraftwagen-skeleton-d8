{
    "name": "drupal-composer",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "support": {},
    "repositories": [
        {
            "type": "composer",
            "url": "https://raw.githubusercontent.com/webflo/d8/master/packages.json"
        },
        {
            "type": "composer",
            "url": "https://packagist.drupal-composer.org"
        }
    ],
    "require": {
        "composer/installers": "1.0.20",
        "netresearch/composer-patches-plugin": "~1.0",
        "drupal/drupal": "8.0.*",

        "drupal/devel": "8.1.*@dev",
        "drupal/token": "8.1.*@dev",

        "drupal/paragraphs": "8.1.0-alpha2"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "preferred-install" : "dist",
    "scripts": {
    },
    "extra": {
        "installer-paths": {
            "core": ["type:drupal-core"],
            "profiles/***MACHINE_NAME***/modules/contrib/{$name}": ["type:drupal-module"],
            "profiles/***MACHINE_NAME***/contrib/{$name}": ["type:drupal-profile"],
            "profiles/***MACHINE_NAME***/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/commands/{$name}": ["type:drupal-drush"]
        },
        "patches": {

        }
    }
}
